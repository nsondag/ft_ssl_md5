md5 -sHello world
echo "-"
./ft_ssl md5 -sHello world
echo "-------------"
echo -n Hello World | openssl md5
echo "-"
echo -n Hello World | ./ft_ssl md5
echo "-------------"
echo -n Hello World | openssl sha256
echo "-"
echo -n Hello World | ./ft_ssl sha256
echo "-------------"
echo "Hello" | ./ft_ssl md5 -s "" -p
echo "-"
echo "Hello" | md5 -s "" -p
echo "-------------"
./ft_ssl md5 -s "ewrwetew" tester/file tester/file rette tester/file
echo "-"
md5 -s "ewrwetew" tester/file tester/file rette tester/file
